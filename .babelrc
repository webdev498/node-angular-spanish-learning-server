{
  "sourceMaps": "inline",
  "presets": [
    "node5"
  ],
  "plugins": [
    "transform-flow-strip-types",
    [
      "module-alias",
      [
        {
          "src": "./src/http",
          "expose": "http"
        },
        {
          "src": "./src/logging",
          "expose": "logging"
        },
        {
          "src": "./src/common/models/Base",
          "expose": "models/Base"
        },
        {
          "src": "./src/users/models/User",
          "expose": "models/User"
        },
        {
          "src": "./src/users/models/Address",
          "expose": "models/Address"
        },
        {
          "src": "./src/users/models/Telephone",
          "expose": "models/Telephone"
        },
        {
          "src": "./src/nationalities/models/Nationality",
          "expose": "models/Nationality"
        },
        {
          "src": "./src/categories/models/Category",
          "expose": "models/Category"
        },
        {
          "src": "./src/security/authorization/models/Role",
          "expose": "models/Role"
        },
        {
          "src": "./src/security/authorization/models/Permission",
          "expose": "models/Permission"
        },
        {
          "src": "./src/security/authorization/models/Operation",
          "expose": "models/Operation"
        },
        {
          "src": "./src/security/authorization/models/Resource",
          "expose": "models/Resource"
        },
        {
          "src": "./src/terminology/models/Term",
          "expose": "models/Term"
        },
        {
          "src": "./src/languages/models/Language",
          "expose": "models/Language"
        },
        {
          "src": "./src/users/service",
          "expose": "users/service"
        },
        {
          "src": "./src/security/authorization/roles",
          "expose": "security/authorization/roles"
        },
        {
          "src": "./src/data/orm",
          "expose": "data/orm"
        },
        {
          "src": "./src/javascript/datatypes/string",
          "expose": "datatypes/string"
        },
        {
          "src": "./src/javascript/datatypes/uuid",
          "expose": "datatypes/uuid"
        },
        {
          "src": "./src/security/authorization/middleware",
          "expose": "authorization/middleware"
        },
        {
          "src": "./src/exceptions/requests/Unauthenticated",
          "expose": "exceptions/requests/Unauthenticated"
        },
        {
          "src": "./src/exceptions/requests/Unauthorized",
          "expose": "exceptions/requests/Unauthorized"
        },
        {
          "src": "./src/exceptions",
          "expose": "exceptions"
        },
        {
          "src": "./etc/testing",
          "expose": "testing"
        },
        {
          "src": "./src/email",
          "expose": "email"
        }
      ]
    ]
  ],
  "env": {
    "production": {
      "ignore": [
        "**/specs/*"
      ]
    }
  },
  "ignore": [
    "node_modules/*",
    "coverage/*"
  ],
  "comments": false,
  "compact": "auto"
}
